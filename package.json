    "dyn-require": "0.0.3",
    "express": "^4.16.3",
    "express-swagger-generator": "^1.1.0",
    "open": "0.0.5"
  },
  "devDependencies": {
    "chalk": "^2.4.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.4",
    "express-swagger-generator": "^1.1.0",
    "localtunnel": "^1.9.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2"
  }
