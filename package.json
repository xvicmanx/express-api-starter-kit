  "description": "A boiler plate project to start coding your Express API right away.",
  "main": "index.js",
  "scripts": {
    "lint": "esw --color src/ scripts/",
    "lint:watch": "npm run lint -- --watch --fix",
    "start": "npm-run-all --parallel start:server lint:watch",
